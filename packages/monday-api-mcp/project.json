{
  "name": "monday-api-mcp",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/monday-api-mcp",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "packages/monday-api-mcp"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm test",
        "cwd": "packages/monday-api-mcp"
      },
      "dependsOn": ["build"]
    }
  },
  "tags": [],
  "implicitDependencies": ["api-types", "setup-api", "api", "agent-toolkit"]
}
