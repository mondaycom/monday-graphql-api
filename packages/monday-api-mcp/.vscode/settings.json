{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.suggest.autoImports": true,
  "typescript.referencesCodeLens.enabled": true,
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "source.organizeImports": "explicit",
    "source.fixAll": "explicit"
  },
  "jestrunner.configPath": "node_modules/@mondaydotcomorg/trident-backend-toolkit/dist/configurations/jest/jest.config.js",
  "jest.jestCommandLine": "node_modules/.bin/jest --config node_modules/@mondaydotcomorg/trident-backend-toolkit/dist/configurations/jest/jest.config.js"
}
